{
    "menu": {
        "item": {
            "label": "$menuitem($t({{labelId}}))",
            "text": "$menuitem({{text}})"
        }
    },
    "modal": {
        "control": {
            "cancel": "Cancel",
            "close": "Close",
            "next": "Next",
            "save": "Save"
        }
    },
    "toolbar": {
        "item": {
            "label": "$t({{labelId}})",
            "label-accelerator": "$t({{labelId}}) ($cmdacc({{commandId}}))",
            "text": "$t({{text}})",
            "text-accelerator": "$t({{text}}) ($cmdacc({{commandId}}))"
        }
    },
    "window": { 
        "title-bar": {
            "auto-hide": {
                "off": "Disable Auto Hide",
                "on": "Enable Auto Hide"
            },
            "close": "Close",
            "position": "Window Position"
        }
    },
    "window-container": {
        "tab": {
            "control": {
                "close": "Close",
                "pin": "Toggle pin status",
                "unpin": "Toggle pin status"
            }
        }
    }
}