{
  "name": "pcxdbg",
  "version": "0.3.0",
  "description": "Multi-platform debugger front-end",
  "homepage": "https://github.com/pcxdbg/pcxdbg",
  "bugs": "https://github.com/pcxdbg/pcxdbg/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pcxdbg/pcxdbg.git"
  },
  "main": "pcxdbg.js",
  "scripts": {
    "start": "npm-run-all --parallel webpack:watch electron:serve",
    "webpack:watch": "webpack --watch",
    "start:web": "webpack-dev-server --content-base . --port 4000 --inline",
    "build:electron:main": "tsc src/pcxdbg.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "build": "webpack --display-error-details && npm run build:electron:main",
    "build:release": "cross-env NODE_ENV=release npm run build",
    "electron:serve": "npm run build:electron:main && electron ./dist --serve",
    "electron:test": "electron ./dist",
    "electron:development": "npm run build && electron ./dist",
    "electron:release": "npm run build:release && electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:win32": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:macos": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "karma start ./karma.conf.js"
  },
  "dependencies": {
    "core-js": "2.4.1",
    "enhanced-resolve": "3.3.0",
    "i18next": "10.0.7",
    "i18next-browser-languagedetector": "2.1.0",
    "i18next-xhr-backend": "1.5.0"
  },
  "devDependencies": {
    "@types/core-js": "0.9.36",
    "@types/i18next": "8.4.2",
    "@types/i18next-xhr-backend": "1.4.1",
    "@types/jasmine": "2.5.54",
    "@types/node": "7.0.7",
    "autoprefixer": "7.1.4",
    "copy-webpack-plugin": "4.2.0",
    "copyfiles": "1.2.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "electron": "1.7.6",
    "electron-packager": "9.1.0",
    "electron-reload": "1.2.1",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.5",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "node-sass": "4.6.0",
    "npm-run-all": "4.1.2",
    "postcss-loader": "2.0.6",
    "postcss-url": "7.1.2",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.1",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "ts-loader": "3.1.1",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typedoc": "0.9.0",
    "typedoc-webpack-plugin": "1.1.4",
    "typescript": "2.5.2",
    "url-loader": "0.5.9",
    "webdriver-manager": "12.0.6",
    "webpack": "3.6.0",
    "webpack-concat-plugin": "1.4.2",
    "webpack-dev-server": "2.8.2"
  }
}
